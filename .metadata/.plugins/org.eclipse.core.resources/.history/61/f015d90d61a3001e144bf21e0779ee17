package com.code;

public class Test {
	public static void main(String[] args) {
		DLInkedList l1 = new DLInkedList();
		l1.insert(10);
		l1.insert(20);
		l1.insert(30);
		l1.insert(40);
		l1.insert(50);
		l1.display();
		l1.insert(1, 60);
		l1.insert(3,70);
		l1.insert(5,80);
		l1.insert(7, 90);
		l1.insert(9, 100);
		l1.display();
		l1.displayRev();
		l1.deleteByVal(100);
		l1.deleteByVal(90);
		l1.deleteByVal(80);
		l1.deleteByPosition(5);
		l1.deleteByPosition(4);
		l1.deleteByPosition(2);
		l1.display();
		l1.displayRev();
	}

}
