package com.code;

public class LinkedList {
	private Node head;

	public Node getHead() {
		return head;
	}

	public LinkedList() {
		head = null;
	}
	
	public boolean insert(int data) {
		Node newNode = new Node(data);
		if(head == null) {
			head = newNode;
		}
		
		Node last = head;
		while(last.getNext()!= null) {
			last = last.getNext();
		}
		last.setNext(newNode);
		return true;
		
	}

}
